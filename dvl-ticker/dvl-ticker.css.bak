html{
   height: 						100%;
   font-family: 			Verdana, Arial, Tahoma, San Serif;
   color: 			 			#fff;
   font-size:					12px;
   line-height:				18px;
   padding:						0px;
   margin:						0px;
}

body{
  background:      #014A9C;
  background: -moz-linear-gradient(top, #014A9C 0%,#014A9C 60%,#5290cc 100% ); /* FF3.6+ */
	background: -webkit-linear-gradient(top,  #014A9C 0%,#014A9C 20%,#5290cc 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #014A9C 0%,#014A9C 20%,#5290cc 100%); /* Opera11.10+ */
	background: -ms-linear-gradient(top,  #014A9C 0%,#014A9C 20%,#5290cc 100%); /* IE10+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#014A9C', endColorstr='#5290cc',GradientType=0 ); /* IE6-9 */
  background: linear-gradient(top,  #014A9C 0%,#014A9C 20%,#5290cc 100%); 
   margin:						0px;
}

.dvl_buttton{
	background:						url(dvl_button.png) transparent;
	width:								159px;
	height:								73px;
	padding:							10px 10px 10px 90px;
	margin:								10px;
  display:							block;
  
   -moz-box-shadow: 			5px 5px 5px #3d3d3d;
  box-shadow: 					5px 5px 5px #3d3d3d; 
}
.dvl_buttton p{
		
	font-size:						24px;
	line-height:					70px;
}

.floatleft{
	float:								left;
}

.floatright{
	float:								right;
}


.dvl_stardesmonats{
	background:						url(dvl_button_stars.png) transparent;
	width:								129px;
	height:								73px;
	padding:							10px 10px 10px 120px;
	margin:								10px;
  display:							block;
  
  -moz-box-shadow: 			5px 5px 5px #3d3d3d;
  box-shadow: 					5px 5px 5px #3d3d3d; 
}


header{
	text-align:						center;
  -moz-box-shadow: 			0px 5px 5px #3d3d3d;
  box-shadow: 					0px 5px 5px #3d3d3d; 
  -moz-border-radius: 	0px;
  border-radius: 				0px;   
  border-bottom:				0px solid #fff;
  margin:								0px 0px 20px 0px;
  padding:							0px;
  height: 							auto;
  background:      			#014A9C;
  background: 					-webkit-linear-gradient(top,  #388EDE 0%,#014A9C 60%,#014A9C 100%); /* Chrome10+,Safari5.1+ */
  text-shadow:					5px 5px #3d3d3d;
  min-width:						950px;
  						
}

header .mitte{
	display:							inline-block;
	width:								auto;
	height:								96px;
	padding:							10px 10px;
	margin:								0px;
	text-shadow:					5px 5px #3d3d3d;
	font-size:						18px;
	line-height:					40px;
}
header .status{
	display:							block;
	width:								auto;
	height:								20px;
	padding:							2px 10px;
	margin:								0px;
	font-size:						10px;
	line-height:					20px;
	background:						#024;
	background: 					-webkit-linear-gradient(top,  #002244 0%,#002244 30%,#111111 100%); /* Chrome10+,Safari5.1+ */

}
.liga{
	height:								20px;
	padding:							2px 10px;
	margin:								0px;
	font-size:						14px;
	line-height:					20px;
	background:						#002244;
	background: 					url(chevron_circle.png),-webkit-linear-gradient(top,  #014A9C 0%,#002244 70%,#002244 100%); /* Chrome10+,Safari5.1+ */
	background: 					url(chevron_circle.png),-moz-linear-gradient(top,  #014A9C 0%,#002244 70%,#002244 100%); /* Chrome10+,Safari5.1+ */
  background-repeat:		no-repeat;
  background-position: 	right center;
	 -moz-box-shadow: 		0px 5px 5px #3d3d3d;
  box-shadow: 					5px 5px 5px #3d3d3d; 
  -moz-border-radius: 	5px;
  border-radius: 				5px;   
  border:								1px solid #fff;
  margin:								0px 0px 10px 0px;
  padding:							6px 10px;
  

}

#links{
   height:							100%;
   width: 							260px;
   padding:							10px;
}


h1{
	font-size:						32px;
  line-height:					40px;
  font-weight: bold;
 

}

p,h1,h2,h3{
	margin:						0px;
}


.icon{
  height: 							50px;
  width:  							50px;
  -moz-border-radius: 	8px;
  border-radius: 				8px;   
  -webkit-transition: all 1s ease-in-out;
	-moz-transition: 			all 1s ease-in-out;
	transition: 					all 1s ease-in-out;
	cursor:								pointer;
	float:								left;
	margin:								5px;
}




.icon:hover{
  -moz-transform: 		scale(1.5) rotate(359.9deg) translate(0px, 0px) skew(0deg, 0deg);
  -webkit-transform:	scale(1.5) rotate(359.9deg) translate(0px, 0px) skew(0deg, 0deg);
	-o-transform:				scale(1.5) rotate(359.9deg) translate(0px, 0px) skew(0deg, 0deg);
	-ms-transform:			scale(1.5) rotate(359.9deg) translate(0px, 0px) skew(0deg, 0deg);
	transform: 					scale(1.5) rotate(359.9deg) translate(0px, 0px) skew(0deg, 0deg);
	-moz-box-shadow: 3px 3px 3px #444444;
  box-shadow: 3px 3px 3px #444444; 
  
}



.desktop_kategorie{
 
  float:left;
  width:48%;
  
  -moz-box-shadow: 5px 5px 5px #3d3d3d;
  box-shadow: 5px 5px 5px #3d3d3d;
  -moz-border-radius: 8px;
  border-radius: 8px;
  
  background: #ffffff;
  
  background: -moz-linear-gradient(top, #5290cc   0%,#fff 25%, #fff 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#5290cc ), color-stop(100%,#003063)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, #5290cc   0%,#fff 30%, #fff 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, #5290cc   0%,#fff 30%, #fff 100%); /* Opera11.10+ */
  background: -ms-linear-gradient(top, #5290cc   0%,#fff 30%, #fff 100%); /* IE10+ */
  /*filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
  background: linear-gradient(top, #5290cc   0%,#fff 30%, #fff 100%); /* W3C */
  
  
  margin: 7px 10px;
  
  padding: 0px 00px;
   text-align: left;
  height:250px;
  overflow: hidden;
  border:              solid 1px #222;
  
}
.desktop_kategorie .header {
   color: #fff; 
   margin:0px 00px 30px 0px; 
   padding: 0px 20px;
    background: -ms-linear-gradient(top, #5290cc   0%,#fff 30%, #fff 100%); /* IE10+ */
   filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#5290cc', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
}